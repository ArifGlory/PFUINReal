            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="myproject.pfuinreal.MenuActivity" />
        <activity
            android:name="myproject.pfuinreal.LatihanActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="myproject.pfuinreal.ResultActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="myproject.pfuinreal.ViewPanduanPraktikum" />
        <activity
            android:name="myproject.pfuinreal.HistoryActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="myproject.pfuinreal.PembahasanActivity" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="myproject.pfuinreal.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>